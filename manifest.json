{
  "name": "Search with Slash (/)",
  "description": "Use your keyboard's slash (/) key to focus on websites' search boxes",
  "manifest_version": 2,
  "version": "0.2",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {"128": "icon_128.png"},
  "permissions": ["input"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}
